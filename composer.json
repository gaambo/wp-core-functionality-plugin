{
  "name": "gaambo/wp-core-functionality-plugin",
  "version": "1.1.1",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "A must-use Plugin for your WordPress website projects to contain all site-specific functionality. This way it is theme independent and always enabled.",
  "homepage": "https://github.com/gaambo/wp-core-functionality-plugin",
  "authors": [
    {
      "name": "Fabian Todt",
      "email": "fabian@fabiantodt.at",
      "homepage": "https://fabiantodt.at"
    }
  ],
  "keywords": [
    "wordpress",
    "wp",
    "core-functionality",
    "must-use plugin"
  ],
  "support": {
    "issues": "https://github.com/gaambo/wp-core-functionality-plugin/issues"
  },
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "repositories": [],
  "require": {
    "php": ">=7.4",
    "composer/installers": "^1.6"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0.2"
  },
  "extra": {},
  "scripts": {
    "test": [
      "phpcs"
    ]
  }
}
